{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/docs/swagger-ui-bundle\\.js",
            "dest": "src/server.js",
            "headers": {
                "Content-Type": "application/javascript",
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/api/docs/swagger-ui-standalone-preset\\.js",
            "dest": "src/server.js",
            "headers": {
                "Content-Type": "application/javascript",
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/api/docs/swagger-ui\\.css",
            "dest": "src/server.js",
            "headers": {
                "Content-Type": "text/css",
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}