{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/docs/(.*)\\.js",
            "dest": "src/server.js",
            "headers": {
                "Content-Type": "application/javascript",
                "Cache-Control": "public, max-age=31536000",
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/api/docs/(.*)\\.css",
            "dest": "src/server.js",
            "headers": {
                "Content-Type": "text/css",
                "Cache-Control": "public, max-age=31536000",
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/api/docs/(.*)",
            "dest": "src/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}